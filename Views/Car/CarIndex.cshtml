<body>
    <div align="center">
        <h2 align="center">Simple Car Finder</h2>
        <p align="center">See if you can find your dream car!</p>
        <img src="data:image/jpeg;base64," alt="Car Image">

        <br />

        <button id="home" type="button">
            Go back to home page
        </button>
        <br />
    </div>
</body>

<script>
    $("#home").click(function() {
        window.location.href="/Home/Index"
    });
</script>

<div id="grid"></div>

<script>
        var carData = [
            {CarID: 1, CarModel: "BMW e30", CarYear: new Date(1990, 1, 1), Price: 5000, Available: false},
            {CarID: 2, CarModel: "Nissan Skyline R32", CarYear: new Date(1992, 1, 1), Price: 12000, Available: false},
            {CarID: 3, CarModel: "Mercedez C63", CarYear: new Date(2016, 1, 1), Price: 70000, Available: true}
        ];

        // custom logic start

        var carDataNextID = carData.length + 1;

        function getIndexById(id) {
            var idx,
                l = carData.length;

            for (var j=0; j < l; j++) {
                if (carData[j].CarID == id) {
                    return j;
                }
            }
            return null;
        }

        // custom logic end

        $(document).ready(function () {
            var dataSource = new kendo.data.DataSource({
                transport: {
                    read: function (e) {
                        // on success
                        e.success(carData);
                    },

                    create: function (e) {
                        // Adding new entry to set ID
                        e.data.CarID = carDataNextID++;
                        // Saving data entry to model
                        carData.push(e.data);

                        e.success(e.data);
                    },

                    update: function (e) {
                        carData[getIndexById(e.data.CarID)] = e.data;

                        e.success();
                    },

                    destroy: function (e) {
                        carData.splice(getIndexById(e.data.CarID), 1);

                        e.success();
                    }
                },

                error: function (e) {
                    alert("Status: " + e.status + "; Error message: " + e.errorThrown);
                },
                pageSize: 10,
                batch: false,
                schema: {
                    model: {
                        id: "CarID",
                        fields: {
                            CarID: { editable: false, nullable: true },
                            CarModel: { validation: { required: true } },
                            CarYear: { type: "date" },
                            Price: { type: "number", validation: { required: true, min: 1} },
                            Available: { type: "boolean" } }
                    }
                }
            });

            $("#grid").kendoGrid({
                dataSource: dataSource,
                pageable: true,
                filterable: true,
                toolbar: ["create"],
                columns: [
                    { field: "CarModel", title: "Car Model" },
                    { field: "CarYear", title: "Car Year", format: "{0:yyyy/MM/dd}", width: "200px" },
                    { field: "Price", title: "Price", format: "{0:c}", width: "120px" },
                    { field: "Available", width: "120px" },
                    { command: ["edit", "destroy"], title: "&nbsp;", width: "200px" }
                ],
                editable: "inline"
            });
        });
</script>
